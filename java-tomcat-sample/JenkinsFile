pipeline {
    agent any
    tools {
        jdk 'JAVA_HOME'
    }
    stages {
        stage('Build application') {
            steps {
                bat "mvn -f java-tomcat-sample/pom.xml clean package"
            }
            post {
                success {
                    echo "Archiving artiffcats"
                    archiveArtifacts artifacts: '**/*.war'
                }
            }
        }
        stage('Deploy to stageenvironemts') {
            steps {
                build job: 'jsp-deploy
            }
        }
    }
}